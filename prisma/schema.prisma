// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:../data/dev.db"
}

generator client {
  provider = "prisma-client-js"
  output   = "./client"
}

model Player {
  id            String @id @default(uuid())
  discordUserId String @unique
  locationId    String
}
